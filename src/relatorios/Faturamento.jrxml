	<queryString>
		<![CDATA[select e.razao_social razao_empresa,e.telefone telefone_empresa,e.endereco endereco_empresa,*,
case

 when fase = 'c' then 'Cancelado'
 when fase = 'e' then 'Encerrado'
end

 from faturamento f, cliente c,empresa e where f.id_cliente = c.id and f.data_emissao >= $P{Data_emissao_inicial}  and f.data_emissao <= $P{Data_emissao_final} and f.fase like '%' ||$P{Fase}||'%' order by f.id]]>
	</queryString>
